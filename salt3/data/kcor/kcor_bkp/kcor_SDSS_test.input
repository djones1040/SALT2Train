SN_SED: Hsiao07.dat
AB_SED:  flatnu.dat
MAGSYSTEM:  AB
FILTSYSTEM: COUNT
FILTPATH:   PS1/Pantheon//SDSS_Doi2010/CCDAVG
FILTER: SDSS-u U.dat 0.00000
FILTER: SDSS-g G.dat -0.0073-0.0029473880
FILTER: SDSS-r R.dat -0.0088+0.0044133478
FILTER: SDSS-i I.dat -0.0090+0.00072395845
FILTER: SDSS-z Z.dat -0.0094-0.0083579659

DUPLICATE_LAMSHIFT_GLOBAL: 10

